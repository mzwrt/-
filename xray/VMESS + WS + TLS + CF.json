{
  "log": {
    "loglevel": "warning",
    "access": "/tmp/xray_log-access.log",
    "error": "/tmp/xray_log-error.logs"
  },
  "inbounds": [
    {
      "listen": "127.0.0.1",
      "port": 4431,
      "protocol": "vmess",
      "settings": {
        "clients": [
          {
            "id": "XXXXX-XXXXX-XXXXX-XXXXX-XXXXX",
            "alterId": 0
          }
        ],
        "disableInsecureEncryption": true
      },
      "streamSettings": {
        "network": "ws",
        "security": "tls",
        "tlsSettings": {
          "serverName": "XXXXX.com",
          "certificates": [
            {
              "certificateFile": "/XXX/fullchain.pem",
              "keyFile": "/XXX/privkey.pem"
            }
          ]
        },
        "wsSettings": {
          "path": "/iclouddrive",
          "headers": {
            "User-Agent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/135.0.0.0 Safari/537.36",
            "Referer": "https://www.icloud.com/",
            "Accept-Language": "zh-CN,zh;q=0.9,en;q=0.8",
            "Accept-Encoding": "gzip, deflate, br, zstd",
            "Connection": "keep-alive",
            "Upgrade-Insecure-Requests": "1",
            "TE": "Trailers",
            "Accept": "text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.7",
            "Cache-Control": "max-age=0",
            "Pragma": "no-cache"
          },
          "wsOptsHeaders": {
            "X-Real-IP": "23.203.211.99",  // 模拟真实的 IP 地址
            "X-Forwarded-For": "23.203.211.99",  // 模拟 X-Forwarded-For
            "X-Frame-Options": "DENY",  // 防止页面被嵌套在iframe中
            "Cache-Control": "no-store"  // 禁用缓存
          }
        }
      },
      "tag": "inbound-4431",
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls"
        ]
      }
    }
  ],
  "outbounds": [
    {
      "tag": "direct",
      "protocol": "freedom"
    },
    {
      "tag": "block",
      "protocol": "blackhole"
    }
  ]
}
